{ 
	"name": "LinkWrench Extension", 
	"description": "LinkWrench Chrome Extension", 
	"version": "0.2", 
	"permissions": ["contextMenus","tabs","webNavigation","storage"], 
	"background": { 
		"persistent": false, 
		"scripts": ["linkwrench.js"] 
	}, 
	"browser_action": {
		"default_popup": "popup.html" 
	},
	"manifest_version": 2,
	"icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" }
}
